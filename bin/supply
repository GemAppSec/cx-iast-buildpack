#!/bin/bash
set -euo pipefail
echo "supply running"
BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
IDX=$4
echo $BUILD_DIR
echo $CACHE_DIR
echo $DEPS_DIR
echo $IDX

BP=$(dirname "$(dirname $0)")
echo $BP
echo $1
which curl
which unzip
which zip
which jq
which sed
echo "environment:"
env


IAST_SERVER=$(jq '.["user-provided"][] | select (.name | contains("checkmarx")).credentials.iast_server')
AGENT_URL="$IAST_SERVER/iast/compilation/download/JAVA"

echo $IAST_SERVER
echo $AGENT_URL

curl -k $AGENT_URL > cx-iast-agent.zip
unzip cx-iast-agent.zip


ls -alR $DEPS_DIR/$IDX
